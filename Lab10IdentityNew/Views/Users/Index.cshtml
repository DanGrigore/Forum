@model Lab10IdentityNew.Models.ApplicationUser

@{


    ViewBag.Title = "Users";
}

<ol class="breadcrumb">
    <li><h3>@ViewBag.Title</h3></li>
</ol>

@foreach (var user in ViewBag.UsersList)
{
    <div class="panel-heading">
        <i>Id: @user.Id</i>
        <br />
        Username: <span class="label label-default">@user.UserName</span>
    </div>
    <div class="panel-footer">
        <a class="btn btn-sm btn-success" href="/Users/Edit/@user.Id">Modify user</a>
    </div>
    <br />
}



